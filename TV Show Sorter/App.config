<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="SearchFolder" value="S:\RenderTool\SearchFolder"/>
    <!--Root destination folder for tv shows-->
    <add key="TVDestinationFolder" value="S:\RenderTool\OutFolder"/>
    <!--Root destination folder for movies-->
    <add key="MovieDestinationFolder" value="S:\RenderTool\Movies"/>
    <!--If EnableOutFolder is true, it will enable the two destination folders above. If set to false, the two paths are ignored.-->
    <add key="EnableOutFolder" value="false"/>
  </appSettings>
</configuration>